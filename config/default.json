{
  "logger": {
    "debugEnabled": 0,
    "logDir": "/opt/kaltura/log",
    "accessLogName": "kaltura_async_proxy_access.log",
    "logName": "kaltura_async_proxy.log",
    "errorLogName": "kaltura_async_proxy_error.log"
  },
  "server": {
    "version": "v1.0"
  },
  "destinationHost": "www.kaltura.com",
  "cloud": {
    "requestTimeout": 100000,
    "httpPort": 8080,
    "httpsPort": 443
  },
  "throttle": {
    //example - 10 messages per second will be [messagePerTimePeriod:10 , timePeriod: 1000]
    "messagePerTimePeriod": 10,
    "timePeriod": 1000
  },
  "handlers": [
    //history example
    {
      "identifiers": [
        {
          "Key": "Service",
          "value": "userEntry"
        },
        {
          "Key": "action",
          "value": "update"
        }
      ],
      "cacheKeys": "cuepointId,userEntry",
      "ttl": 30,
      "extendTtl": true
    },
    //aggregation example
    {
      "identifiers": [
        {
          "Key": "Service",
          "value": "Cuepoint"
        },
        {
          "Key": "action",
          "value": "update"
        }
      ],
      "cacheKeys": "cuepointId, partnerId",
      "ttl": 30,
      "extendTtl": false
    }
  ]
}
